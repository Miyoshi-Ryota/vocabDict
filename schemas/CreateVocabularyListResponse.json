{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "CreateVocabularyListResponse",
  "description": "Response from creating a new vocabulary list.",
  "type": "object",
  "required": ["success"],
  "properties": {
    "success": {
      "type": "boolean",
      "description": "Whether the list was successfully created."
    },
    "vocabularyList": {
      "type": "object",
      "description": "The created vocabulary list.",
      "properties": {
        "id": {
          "type": "string",
          "format": "uuid"
        },
        "name": {
          "type": "string"
        },
        "isDefault": {
          "type": "boolean"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "error": {
      "type": "string",
      "description": "Error message if the operation failed."
    }
  }
}