{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "FetchSettingsRequest",
  "description": "Request to fetch user settings.",
  "type": "object",
  "required": ["action"],
  "additionalProperties": false,
  "properties": {
    "action": {
      "type": "string",
      "const": "fetchSettings",
      "description": "The action to be performed. Must be 'fetchSettings'."
    }
  }
}
