{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "FetchRecentSearchesResponse",
  "description": "Response containing recent searches.",
  "type": "object",
  "required": ["success"],
  "additionalProperties": false,
  "properties": {
    "success": {
      "type": "boolean",
      "description": "Whether the fetch was successful."
    },
    "recentSearches": {
      "type": "array",
      "description": "Array of recent search words.",
      "items": {
        "type": "string"
      }
    },
    "error": {
      "type": "string",
      "description": "Error message if the operation failed."
    }
  }
}
